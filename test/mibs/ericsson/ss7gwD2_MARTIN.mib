	ss7GwRasReceiver
		OBJECT IDENTIFIER ::= { ss7GatewayD2 5 }

	ss7GwswMgmt
		OBJECT IDENTIFIER ::= { ss7GatewayD2 6 }

	ss7GwSecurity
		OBJECT IDENTIFIER ::= { ss7GatewayD2 7 }

	mtp
		OBJECT IDENTIFIER ::= { ss7GatewayD2 10 }

	isup
		OBJECT IDENTIFIER ::= { ss7GatewayD2 11 }

	sccp
		OBJECT IDENTIFIER ::= { ss7GatewayD2 12 }

	tcapRelay
		OBJECT IDENTIFIER ::= { ss7GatewayD2 13 }

	ss7GwTraps
		OBJECT IDENTIFIER ::= { ss7GatewayD2 14 }

--	ss7gwTrapReceiverTable		ss7GatewayD2  4
--	ss7gwPhysNodeCfgTable   	ss7GatewayD2  8
--	ss7gwPhysNodeStatusTable 	ss7GatewayD2  9


-- Definition of a Boolean value
	Boolean ::= INTEGER { false(0), true(1) }
-- Definition of a string
	DisplayString ::= OCTET STRING

--===============================================================
-- SS7 Gateway MIB Objects
--===============================================================
-- GENERAL CONFIG ATTRIBUTES
--===========================

	opc OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..11))
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Own Point Code (OPC) for the SS7 Gateway
			entered in the format XXX-XXX-XXX where X is
			a value from 0 to 9"
                                                
		::= { ss7GwGeneralCfg 1}

        ss7gwNodeId OBJECT-TYPE 
                SYNTAX  DisplayString (SIZE (0..20))
                ACCESS  read-write
                STATUS  mandatory
                DESCRIPTION
                        "The Node Identification of the SS7 Gateway." 
                                                
                ::= { ss7GwGeneralCfg 2}

        ss7gwMtpFlavour OBJECT-TYPE 
                SYNTAX  INTEGER 
			{
                           ituMTP(1),
                           ansiMTP(2)
                        }
                ACCESS  read-write
                STATUS mandatory
                DESCRIPTION
                        "The flavour of the MTP stack."
                                                
                ::= { ss7GwGeneralCfg 3}

        ss7gIsupFlavour OBJECT-TYPE 
                SYNTAX  INTEGER 
			{
                           ituISUP(1),
                           ansiISUP(2)
                        }
                ACCESS  read-write
                STATUS mandatory
                DESCRIPTION
                        "The flavour of the ISUP stack."
                                                
                ::= { ss7GwGeneralCfg 4}

	systemType	 OBJECT-TYPE
                SYNTAX  INTEGER 
			{
                           singleBoard(1),
                           multiBoard(2),
			   haConfiguration(3)
                        }
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the configuration of the SS7GW."
                                                
		::= { ss7GwGeneralCfg 5}


--===========================================================
	flashDbConsistency OBJECT-TYPE
		SYNTAX  Boolean
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the consistency of all data located in 
			database and all data located in Flash.  If set to
			true, all data in database (in effect or not) matches 
			the data stored in Flash memory"
                                                
		::= { ss7GwGeneralStatus 1}

	configDataInUse OBJECT-TYPE
		SYNTAX  Boolean
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the consistency of configuration data 
			between data in use by the system and data stored
			in the configuration database.  If set to true, all
			configuration data stored in the database is in use
			by the system"
                                                
		::= { ss7GwGeneralStatus 2}

	commandSS7gw OBJECT-TYPE
		SYNTAX  INTEGER
		{
		   start(1),
		   stop(2),
		   reboot(3),
		   saveDatatoFlash(4)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"commands to be issued on SS7 Gateway"
                                                
		::= { ss7GwGeneralStatus 3}

	statusSS7gw OBJECT-TYPE
		SYNTAX  INTEGER
		{
		   stop(1),
		   startPending(2),
		   stopPending(3),
		   run(4),
		   startFailed(5)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"status of the SS7 Gateway"
                                                
		::= { ss7GwGeneralStatus 4}

--=========================================================

	ss7gwTrapReceiverTable OBJECT-TYPE
		SYNTAX SEQUENCE OF SS7GwTrapReceiverEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table holds information about the Receivers
			of Traps sent from the SS7 Gateway.

			Rows are added in this table by setting the rowStatus
			variable to Create and by specifying a valid value 
			for all other variables.  All variables must be
			specified in the SNMP-Set operation in order 
			for a row to be created.  

			Rows are deleted in this table by setting the
			rowStatus variable to Destroy.
			
			Rows can be updated in this table by setting the
			rowStatus variable to Update and modifying the
			variables that need changed.  This is to be done in the 
			same SNMP-Set request."

		::= { ss7GatewayD2 4}

	ss7gwTrapReceiverEntry OBJECT-TYPE
		SYNTAX SS7GwTrapReceiverEntry 
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
		    "An entry in the ss7gwTrapReceiver table."
		INDEX { trapReceiverIpAddressIndex}
		::= { ss7gwTrapReceiverTable 1 }

	SS7GwTrapReceiverEntry ::=
		SEQUENCE
		{
			trapReceiverIpAddressIndex
				IpAddress,
			ss7gwTrapReceiverRowStatus
				INTEGER,
			trapCommunityString
				DisplayString,
			trapPort
				INTEGER
		}


	trapReceiverIpAddressIndex OBJECT-TYPE
		SYNTAX  IpAddress
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"IP address of the manager to send a alarm to"
                                                
		::= { ss7gwTrapReceiverEntry 1}

	ss7gwTrapReceiverRowStatus OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   create(1),
			   destroy(2),
			   update(3),
			   active(4),
			   rejected(6)
			}

		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The status of the row.
			The only valid writeable values are Create, Destroy 
			and Update. These are explained in the Table Definition.
			All other values are R/O and will be updated internally
			by the system. 
			Active - all variables in this table are in use by
			         the system
			Rejected - a variable was changed in this table
				   and has an invalid value.  Re-enter the
				   last variable changed or delete the Row."
                                                
		::= { ss7gwTrapReceiverEntry 2}

	trapCommunityString OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..31))
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"community name to use in the alarm (trap) sent to 
			the manager."
                                                
		::= { ss7gwTrapReceiverEntry 3}

	trapPort OBJECT-TYPE
		SYNTAX  INTEGER(0..32767)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"UDP port number used for sending SNMP Trap messages"
                                                
		::= { ss7gwTrapReceiverEntry 4}
--===========================================================
	rasActive OBJECT-TYPE
		SYNTAX  Boolean
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"flag indicating if RAS signalling is to be performed
			(0 - no signalling, 1 -signalling on)"
                                                
		::= { ss7GwRasReceiver 1}

	rasMgtSvrAddr OBJECT-TYPE
		SYNTAX  IpAddress
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"IP address of management server"
                                                
		::= { ss7GwRasReceiver 2}

	rasRemPortNum OBJECT-TYPE
		SYNTAX  INTEGER(0..32767)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Remote Port Number"
                                                
		::= { ss7GwRasReceiver 3}

	rasLocPortNum OBJECT-TYPE
		SYNTAX  INTEGER(0..32767)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Local Port Number"
                                                
		::= { ss7GwRasReceiver 4}

	rasRegInt OBJECT-TYPE
		SYNTAX  INTEGER(1..65535)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Default Registration.  registration interval in
			 seconds (used at startup, management server may reset 
			 this value upon successful registration)"
                                                
		::= { ss7GwRasReceiver 5}

--===========================================================
	ss7gwGeneralAlarmStatus OBJECT-TYPE
		SYNTAX INTEGER {
			noAlarm(0),
                        s7syAlarmS7Traffic(1),
                        s7alAlarmRasAlarm(2),
                        s7isAlarmVgwUnavailable(4),
                        s7smAlarmSccpUserStatus(8),
                        s7pqAlarmMtp1Alarm(16),
                        s7sdAlarmMtp2Alarm(32),
                        s7nmAlarmMtp3Alarm(64),
			s7nmAlarmLinksDown(128),
			s7nmAlarmDPCsUnavailable(256),
                        s7pqFirstE1T1OOS(512),
                        s7pqSecondE1T1OOS(1024),
			s7trTcpTrafficConnDown(2048),
			s7trBindUnsucessful(4096)
			}
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"General alarm status which can be a combination of
                         the following values:
                         1    - Traffic Stopped
                         2    - No Connection to Sitekeeper
                         4    - Connection to Voice Gateway(s) lost
                         8    - SCCP User(s) unavailable
                         16   - MTP Layer 1 Alarm(s)
                         32   - MTP Layer 2 Data Link(s) Down
                         64   - MTP Layer 3 Alarm(s)
                         128  - Link(s) Down
                         256  - Destination Point Code(s) Unavailable
                         512  - First E1/T1 Out Of Service
                         1024 - Second E1/T1 Out Of Service
			 2048 - Tcap Relay Traffic Connection Down
			 4096 - Tcap Relay Bind Unsuccessful"
                                                
	::= { ss7GwAlarm 1}

	ss7gwGeneralEventStatus OBJECT-TYPE
		SYNTAX INTEGER {
			noEvent(0),
                        s7syEventMainChecksumErr(1),
                        s7syEventBakChecksumErr(2),
                        s7syEventNodeRestart(4),
                        s7syEventTrafficStart(8),
                        s7syEventTrafficStop(16),
                        s7alEventAlarmManagerEvent(32),
                        s7isEventIsupEvent(64),
                        s7smEventSccpStackError(128),
                        s7smEventSccpInvalidNetworkMessage(256),
                        s7pqEventMtp1StackError(512),
                        s7sdEventMtp2StackError(1024),
                        s7nmEventMtp3StackError(2048),
                        s7syEventDbReadErr(4096),
                        s7syEventOSReptErr(8192),
                        s7syEventApplicErr(16384),
                        s7syEventRequestToProtocolLayerFailed(32768),
			s7trRegMsgReqSuccess(65536),
			s7trRegMsgReqError(131072),
			s7trDeregMsgReqSuccess(262144),
			s7trDeregMsgReqError(524288),
			s7trConfMsgDiscarded(1048576),
			s7trConfConnAbnClosed(2097152),
			s7trTraffMsgDiscarded(4194304),
			s7trTraffMsgRetNAC(8388608)
                        }
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"General event status which can be a combination of
			the following values:
                        1     - Main Safestore Checksum Error
                        2     - Backup Safestore Checksum Error
                        4     - Node Restarted
                        8     - Traffic Start Ordered
                        16    - Traffic Stop Ordered
                        32    - Unauthorized SNMP Set Request
                        64    - ISUP Event
                        128   - SCCP Stack Error
                        256   - SCCP Invalid Network Message
                        512   - MTP Layer 1 Stack Error
                        1024  - MTP Layer 2 Stack Error
                        2048  - MTP Layer 3 Stack Error
                        4096  - Database Read Error
                        8192  - Error Detected by Operating System
                        16384 - Error Detected by Application
                        32767 - Request to Protocol Layer Failed
			65536 - Tcap Relay Registration Message Request Success
			131072- Tcap Relay Registration Message Request Error
			262144- Tcap Deregistration Message Request Success
			524288- Tcap Relay Deregistration Message Request Error
			1048576-s7trConfMsgDiscarded
			2097152-s7trConfConnAbnClosed
			4194304-s7trTraffMsgDiscarded
			8388608-s7trTraffMsgRetNAC"
                                                
	::= { ss7GwAlarm 2}

	clearEventStatus OBJECT-TYPE
		SYNTAX  INTEGER
		{
		   clearGeneralEventStatus(1)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"when set, General Event Status is cleared."
                                                
		::= { ss7GwAlarm 3}

	ss7gwGeneralLogTxt OBJECT-TYPE
		SYNTAX DisplayString (SIZE (0..255))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"most recent log entry text string, 
			used to provide additional information
			for ss7GwTraps"
                                                
		::= { ss7GwAlarm 4}


--===========================================================
	ss7GwswDwnLdMgmt
		OBJECT IDENTIFIER ::= { ss7GwswMgmt 1 }

	swServerAddress OBJECT-TYPE
		SYNTAX DisplayString (SIZE (0..16))
		ACCESS read-write
		STATUS mandatory
		DESCRIPTION
		"Address to software download server"
                                                
		::= { ss7GwswDwnLdMgmt 1}

	swImagePath OBJECT-TYPE
		SYNTAX DisplayString (SIZE (0..70))
		ACCESS read-write
		STATUS mandatory
		DESCRIPTION
		"Path and filename of image to download."
                                                
		::= { ss7GwswDwnLdMgmt 2}

	swImageSize OBJECT-TYPE
		SYNTAX INTEGER 
		ACCESS read-write
		STATUS mandatory
		DESCRIPTION
		"Size of the download image in bytes."
                                                
		::= { ss7GwswDwnLdMgmt 3}

	swPerformDownload OBJECT-TYPE
		SYNTAX INTEGER {
			performDownLoad(1)
		}
		ACCESS read-write
		STATUS mandatory
		DESCRIPTION
		"Start download of new software when variable is set. 
		 ServerAddress, ImagePath and ImageSize should be filled in 
		 before this function is called."
                                                
		::= { ss7GwswDwnLdMgmt 4}

	pNodeId OBJECT-TYPE
		SYNTAX	INTEGER {
			 allNodes(255)
		}
		ACCESS	read-write
		STATUS mandatory
		DESCRIPTION
		"id of the physical node in which the software will be 
		downloaded to.  Valid values are 1-8, or 255 for all Nodes"
                                                
		::= { ss7GwswDwnLdMgmt 5}
--===========================================================
	ss7gwSwStatusTable OBJECT-TYPE
		SYNTAX SEQUENCE OF SS7GwSwStatusEntry
		ACCESS  not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table contains status information of the software 
			that is The number of entries
			is given by the value of swSoftwareTableSize located in
			the ss7GwswDwnLdMgmt Group."
		::= { ss7GwswMgmt 2}

	ss7gwSwStatusEntry OBJECT-TYPE
		SYNTAX SS7GwSwStatusEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"A ss7gwSwStatusEntry entry containing 
			information about stored software."
		INDEX	{ softwarePNodeIdIndex, softwareIndex }
		::= { ss7gwSwStatusTable 1}

	SS7GwSwStatusEntry ::=
		SEQUENCE {
			softwarePNodeIdIndex
				INTEGER,
			softwareIndex
				INTEGER,
			softwareVersion
				DisplayString (SIZE (0..20)),
			softwareStatus
				INTEGER,
			softwareUpgradeStatus
				INTEGER,
			softwareUpgradeProgress
				INTEGER(0..100),
			softwareUpgradeResult
				INTEGER
		}
	
	softwarePNodeIdIndex OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"Physical Node ID"
                                                
		::= { ss7gwSwStatusEntry 1 }

	softwareIndex OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"A unique value for each entry."
                                                
		::= { ss7gwSwStatusEntry 2 }

        softwareVersion OBJECT-TYPE 
                SYNTAX  DisplayString (SIZE (0..20))
                ACCESS  read-only
                STATUS  mandatory
                DESCRIPTION
                        "The software version of the SS7 Gateway."
                                                
                ::= { ss7gwSwStatusEntry 3}

	softwareStatus OBJECT-TYPE
		SYNTAX INTEGER {
			active(1),
			standby(2),
			corruptEmpty(3)
		}
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		"Corrupt_empty indicates that flash area does not 
		contain a valid image, the two other states indicate
		valid images. Active means that this software is the
		currently running, standby is not running."
                                                
		::= { ss7gwSwStatusEntry 4}

	softwareUpgradeStatus OBJECT-TYPE
		SYNTAX INTEGER {
			noActivity(1),
			downloadConnecting(2),
			downloading(3),
			savingToFlash(4)
		}
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		"Upgrade status for this program area, will indicate the
		current phase."
                                                
		::= { ss7gwSwStatusEntry 5}

	softwareUpgradeProgress OBJECT-TYPE
		SYNTAX INTEGER(0..100)
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		"Progress info used when softwareUpgradeStatus is 
		downloading or savingToFlash. Range is 0 to 100."
                                                
		::= { ss7gwSwStatusEntry 6}
	
	softwareUpgradeResult OBJECT-TYPE
		SYNTAX INTEGER {
			resOk(0),
			resCouldNotConnect(1),
			resTransferFailed(2),
			resUnpackFailed(3),
			resSaveFailed(4),
			resImageCorrupt(5)
		}
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		"Result of the last software upgrade."
                                                
		::= { ss7gwSwStatusEntry 7}
--!!=========================================================

--      SECURITY
--!!=========================================================

	ss7gwRandomNumber OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
		"Random number used for challenge-response."
                                
		::= { ss7GwSecurity 1}
		
	ss7gwMd5Result OBJECT-TYPE
		SYNTAX DisplayString
		ACCESS read-write
		STATUS mandatory
		DESCRIPTION
		"Md5 Result used for challenge-response."
                                                                
		::= {ss7GwSecurity 2}




--===========================================================
	ss7gwPhysNodeCfgTable OBJECT-TYPE
		SYNTAX SEQUENCE OF SS7GwPhysNodeCfgEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table holds information about the physical 
			nodes of the system and what software is running 
			on them.
			Rows cannot be created nor deleted in this table."

		::= { ss7GatewayD2 8}

	ss7gwPhysNodeCfgEntry OBJECT-TYPE
		SYNTAX SS7GwPhysNodeCfgEntry 
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
		    "An entry in the ss7gwPhysNodeCfg table."
		INDEX { ss7gwPnodeCfgIndex}
		::= { ss7gwPhysNodeCfgTable 1 }

	SS7GwPhysNodeCfgEntry ::=
		SEQUENCE
		{
	        	ss7gwPnodeCfgIndex
				INTEGER(1..8),
			ss7gwPnodeName
				DisplayString (SIZE (0..20)),
			ss7gwPnodeIpAddress
				IpAddress,
			pNodeDefaultState
				INTEGER,
			activeFlash
				INTEGER,
			timeFlashUpdated
				DisplayString (SIZE (0..19)),
			swVersionRunning
				DisplayString (SIZE (0..20)),
			oamFuncSupp
				Boolean,
			ss7LinksSupp
				Boolean,
			ss7NWSupp
				Boolean,
			ss7sccpSupp
				Boolean,
			isupGwSupp
				Boolean,
			tcapRelaySupp
				Boolean

		}

	ss7gwPnodeCfgIndex OBJECT-TYPE
		SYNTAX  INTEGER(1..8)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Physical Node Id"
                                                
		::= { ss7gwPhysNodeCfgEntry 1}

	ss7gwPnodeName OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..20))
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"symbolic name for the Physical Node"
                                                
		::= { ss7gwPhysNodeCfgEntry 2}

	ss7gwPnodeIpAddress OBJECT-TYPE
		SYNTAX  IpAddress
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"IP address of the physical node"
                                                
		::= { ss7gwPhysNodeCfgEntry 3}

	pNodeDefaultState OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   outOfService(2),
			   active(4),
			   standby(6)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Default state of the Physical Node"
                                                
		::= { ss7gwPhysNodeCfgEntry 4}

	activeFlash OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   flashArea1(0),
			   flashArea2(1)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"active flash in use/to be used after next reboot"
                                                
		::= { ss7gwPhysNodeCfgEntry 5}

	timeFlashUpdated OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..19))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Time of last flash update. Format used is HH:MM.SS."
                                                
		::= { ss7gwPhysNodeCfgEntry 6}

	swVersionRunning OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..20))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"software version running on the physical node"
                                                
		::= { ss7gwPhysNodeCfgEntry 7}

	oamFuncSupp OBJECT-TYPE
		SYNTAX  Boolean
		ACCESS  read-only
