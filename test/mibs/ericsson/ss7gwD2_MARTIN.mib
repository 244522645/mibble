--===========================================================
	isupRouteTable OBJECT-TYPE
		SYNTAX SEQUENCE OF ISUPRouteEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table holds the Route data. 
                         There is a row for each Voice Gateway (VG) connected 
                         to the SS7 Gateway (SS7GW).

                         Each VG consists on the attributes: Ni, Spc, 
                         SigPointId, and IpAddr, and a set of ranges of Circuit
                         Identification Codes (CICs), each range of CICs 
                         consists on attributes: LowestCic and HighestCic.

                         When adding a VG for first time, all attributes in 
                         current table are set.  A new row is also 
                         created in following tables: ISUPConnectionTable, 
                         and ISUPStatusTable.

                         When adding a new CIC range to an existing VG, a new 
                         row is created with the specified VGIdindex, a new 
                         CicIndex, and the attributes: NiIndex, SpcIndex, 
                         LowestCic and HighestCic. The attribute IpAddr is 
                         optional. No new rows are created in other tables.
                         If the data setting is successful, the RowStatus 
                         is set to active state.

                         A VG connected to different Ni/Spc's has the same 
                         VGIdindex.

                         When deleting a CIC range from an existing VG, the 
                         row matching the specified VGIdindex, NiIndex, 
                         SpcIndex, and isupRouteLowCicIndex, is destroyed.
                         If there is only one row for an existing VG and it 
                         is deleted, the row matching the specified VGIdindex, 
                         NiIndex, SpcIndex, and isupRouteLowCicIndex, is
			 destroyed. Also, 
                         all rows related to the VG in the following tables 
                         are deleted too: ISUPConnectionTable, and 
                         ISUPStatusTable.

                         It is NOT possible to change a single attribute in 
                         a row."
		::= { isup 2}

	isupRouteEntry OBJECT-TYPE
		SYNTAX ISUPRouteEntry 
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
		    "An entry in the isupRoute table."
		INDEX {isupRouteVgIdIndex, isupRouteNiIndex, isupRouteSpcIndex,
 		       isupRouteLowCicIndex}
		::= { isupRouteTable 1 }

	ISUPRouteEntry ::=
		SEQUENCE
		{
			isupRouteVgIdIndex
				DisplayString (SIZE (0..20)),
			isupRouteNiIndex
				INTEGER(0..3),
			isupRouteSpcIndex
				DisplayString (SIZE (0..11)),
			isupRouteLowCicIndex
				INTEGER(0..65535),
			isupRouteHighCic
				INTEGER(0..65535),
			isupRouteIpAddr
				IpAddress,
			isupRouteRowStatus
				INTEGER
		}

	isupRouteVgIdIndex OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..20))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The identification of the Voice Gateway index"
                                                
		::= { isupRouteEntry 1}

	isupRouteNiIndex OBJECT-TYPE
		SYNTAX  INTEGER(0..3)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The Network Indicator parameter included in a message."
                                                
		::= { isupRouteEntry 2}

	isupRouteSpcIndex OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..11))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The Signalling Point Code identifying the Signalling 
			Point."
                                                
		::= { isupRouteEntry 3}

	isupRouteLowCicIndex OBJECT-TYPE
		SYNTAX  INTEGER(0..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The lowest Circuit Identification Code of the range."
                                                
		::= { isupRouteEntry 4}

	isupRouteHighCic OBJECT-TYPE
		SYNTAX  INTEGER(0..65535)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The highest Circuit Identification Code of the range."
                                                
		::= { isupRouteEntry 5}

	isupRouteIpAddr OBJECT-TYPE
		SYNTAX  IpAddress
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The IP address of the destination host (i.e., the 
			Voice Gateway). Messages whose originating Signalling 
			Point is Ni - Spc, and the CIC is included in any 
			defined range of CICs."
                                                
		::= { isupRouteEntry 6}

	isupRouteRowStatus OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   create(1),
			   destroy(2),
			   active(4)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The status of the row.
			The only valid writeable values are Create, Destroy 
			and Active. These are explained in the Table 
			Definition. 
			All other values are R/O and will be updated internally
			by the system. 
			Active - all variables in this table are in use by
			         the system"
                                                
		::= { isupRouteEntry 7}

--===========================================================
	isupConnectionTable OBJECT-TYPE
		SYNTAX SEQUENCE OF ISUPConnectionEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table holds the information related to the 
			supervision of each TCP/IP connection between the 
			ISUP Application and the Voice Gateway.

			Rows cannot be created nor destroyed in this table.

			Rows are automatically added whenever a row in the
			isupRouteTable is added"
		::= { isup 3}

	isupConnectionEntry OBJECT-TYPE
		SYNTAX ISUPConnectionEntry 
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
		    "An entry in the isupConnection table."
		INDEX {isupConnectionVgIdIndex }
		::= { isupConnectionTable 1 }

	ISUPConnectionEntry ::=
		SEQUENCE
		{
			isupConnectionVgIdIndex
				DisplayString (SIZE (0..20)),
			isupheartBeatActivation
				INTEGER,
			isupheartBeatTimer
				INTEGER,
			isupheartBeatThreshold
				INTEGER
		}

	isupConnectionVgIdIndex OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..20))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The identification of the Voice Gateway index"
                                                
		::= { isupConnectionEntry 1}

	isupheartBeatActivation OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   disable(0),
			   enable(1)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This parameter controls if the heart beat function 
			is enabled or disabled. "
		DEFVAL { 1 }
                                                
		::= { isupConnectionEntry 2}

	isupheartBeatTimer OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The step, in milliseconds, to send a heartbeat 
			message to supervise the TCP connection.
			It has meaning when heartBeatActivation is enable"
		DEFVAL { 1000 }
                                                
		::= { isupConnectionEntry 3}

	isupheartBeatThreshold OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Number of lost heartbeats to consider the TCP 
			connection is broken.
			It has meaning when heartBeatActivation is enable"
		DEFVAL { 3 }
                                                
		::= { isupConnectionEntry 4}
