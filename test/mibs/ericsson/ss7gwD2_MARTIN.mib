		FROM RFC1155-SMI
	Counter
		FROM RFC1155-SMI
	enterprises
		FROM RFC1155-SMI
	OBJECT-TYPE
		FROM RFC-1212
	TRAP-TYPE
		FROM RFC-1215;
		
	ericsson			
		OBJECT IDENTIFIER ::= { enterprises 193 }

	ss7gateway
		OBJECT IDENTIFIER ::= { ericsson 49 } 

	ss7GatewayD2			
		OBJECT IDENTIFIER ::= { ss7gateway 2 }

