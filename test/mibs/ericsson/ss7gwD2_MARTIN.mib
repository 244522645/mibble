	sccpAddressTranslationTable OBJECT-TYPE
		SYNTAX SEQUENCE OF SCCPAddressTranslationEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table contains the Address Translation 
			Data.  
			Rows are added in this table by setting the rowStatus
			variable to Create and by specifying a valid value 
			for all other variables.  All variables must be
			specified in the SNMP-Set operation in order 
			for a row to be created.  
			Rows are deleted in this table by setting the
			rowStatus variable to Destroy.  
			Rows can be updated in this table by setting the
			rowStatus variable to Update and by modifying the
			variables that need changed in that row."
		::= { sccp 1}

	sccpAddressTranslationEntry OBJECT-TYPE
		SYNTAX SCCPAddressTranslationEntry 
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
		    "An entry in the sccpAddressTranslation table."
		INDEX { ttIndex, npIndex, naIndex, nsIndex}
		::= { sccpAddressTranslationTable 1 }

	SCCPAddressTranslationEntry ::=
		SEQUENCE
		{
	        	ttIndex
				INTEGER(0..255),
			npIndex
				INTEGER,
			naIndex
				INTEGER,
			nsIndex
				DisplayString (SIZE (1..18)),
			sccpAddressTranslationRowStatus
				INTEGER,
			sccpAddressTranslationReasonCode
				INTEGER,
			dpc
				DisplayString (SIZE (0..11)),
			ssn
				INTEGER
		}

	ttIndex OBJECT-TYPE
		SYNTAX  INTEGER(0..255)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"translation type Index"
                                                
		::= { sccpAddressTranslationEntry 1}

	npIndex OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   notApplicable(255)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"numbering plan Index. valid range (0..14)"
                                                
		::= { sccpAddressTranslationEntry 2}

	naIndex OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   notApplicable(255)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"nature of address Index. valid range (0..63)"
                                                
		::= { sccpAddressTranslationEntry 3}

	nsIndex OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (1..18))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"number series Index"
                                                
		::= { sccpAddressTranslationEntry 4}

	sccpAddressTranslationRowStatus OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   create(1),
			   destroy(2),
			   active(4),
			   pending(5),
			   rejected(6)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The status of the row.
			The only valid writeable values are Create, Destroy 
			and Update. These are explained in the Table Definition. 
			All other values are R/O and will be updated internally
			by the system. 
			Active - all variables in this table are in use by
			         the system
			Rejected - a variable was changed in this table
				   before the last reboot.  That change was 
				   rejected by the system after the reboot. 
				   The variable needs to be retored to it's
				   previous value."
                                                
		::= { sccpAddressTranslationEntry 5}

	sccpAddressTranslationReasonCode OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   ok(0)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"reason for rejection of row by system.  
			This variable only has meaning when the RowStatus 
			is Rejected"
                                                
		::= { sccpAddressTranslationEntry 6}

	dpc OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..11))
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"destination point code"
                                                
		::= { sccpAddressTranslationEntry 7}

	ssn OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   noSSN(65535)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"subsystem number located in dpc.
			If ssn = noSSN, then data in this row is for dpc only."
                                                
		::= { sccpAddressTranslationEntry 8}
--===========================================================
	sccpRouteTable OBJECT-TYPE
		SYNTAX SEQUENCE OF SCCPRouteEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table contains the routing data for SCCP.  
			This table contains a Master Row.
			The Master Row in this table is defined as a row that
			has the value (noSSN) in the ssnIndex field. The
			Master Row controls when the data associated with a 
			dpcIndex in this table and the dpcIndex in the 
			sccpRemoteCpcTable is defined in SCCP.  In order to 
			update any data associated with the Master Row, first, 
			change any data that needs to be updated in this
			Table or the the sccpCpcTable, then set the 
			sccpRouteRowStatus variable to Update.
			At that time, all data associated with the Master
			Row in both tables will be updated in the system.
			There can only be one Master Row per dpcIndex in 
			this table.
			Rows are added in this table by setting the rowStatus
			variable to Create and by specifying a valid value 
			for all other variables.  All variables must be
			specified in the SNMP-Set operation in order 
			for a row to be created.  
			Rows are deleted in this table by setting the
			rowStatus variable to Destroy.  When a Destroy is
			executed, all data associated with this row in other
			tables will also be deleted"
		::= { sccp 2}

	sccpRouteEntry OBJECT-TYPE
		SYNTAX SCCPRouteEntry 
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
		    "An entry in the sccpRoute table."
		INDEX { sccpRouteDpcIndex, sccpRouteSsnIndex}
		::= { sccpRouteTable 1 }

	SCCPRouteEntry ::=
		SEQUENCE
		{
	        	sccpRouteDpcIndex
				DisplayString (SIZE (0..11)),
			sccpRouteSsnIndex
				INTEGER,
			sccpRouteRowStatus
				INTEGER,
			sccpRouteReasonCode
				INTEGER,
			sccpRoutessnName
				DisplayString (SIZE (0..20)),
			sccpRoutebpc
				DisplayString (SIZE (0..11))
		}

	sccpRouteDpcIndex OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..11))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"translation type Index"
                                                
		::= { sccpRouteEntry 1}

	sccpRouteSsnIndex OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   noSSN(65535)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"numbering plan Index"
                                                
		::= { sccpRouteEntry 2}

	sccpRouteRowStatus OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   create(1),
			   destroy(2),
			   update(3),
			   active(4),
			   pending(5),
			   rejected(6),
			   deletepending(7),
			   execute(8)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The status of the row.
			The only valid writeable values are Create, Destroy 
			and Update. These are explained in the Table Definition.

			All other values are R/O and will be updated internally
			by the system. 
			Active - all variables in this table are in use by
			         the system
			Pending - a variable has been changed in this table
				  and is not currently in use by the system.
				  The system must be rebooted before the
				  variable will take effect in the system.
			Rejected - a variable was changed in this table
				   before the last reboot.  That change was 
				   rejected by the system after the reboot. 
				   The variable needs to be retored to it's
				   previous value.
			deletePending - The Rowstatus variable was set to 
					delete, but a reboot of the system is
					required before this row is removed
					from the system.

			ssnName is not required for Row Creation but all other
			variables are."
                                                
		::= { sccpRouteEntry 3}

	sccpRouteReasonCode OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   ok(0)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"reason for rejection of all data associated with 
			Master row.  This variable only has meaning for the 
			Master row and when the RowStatus is Rejected"
                                                
		::= { sccpRouteEntry 4}

	sccpRoutessnName OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..20))
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"subsystem name. 
			If sccpRouteSsnIndex is noSSN, then the recomended value
			for this field is '-'"
                                                
		::= { sccpRouteEntry 5}

	sccpRoutebpc OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..11))
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"backup point code.  
			If sccpRouteSsnIndex is noSSN, then this is a bpc to the
			dpc.  Otherwise, this is a bpc to the dpc and ssn"
                                                
		::= { sccpRouteEntry 6}

