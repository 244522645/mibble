--===========================================================
	mtp1TimeSlotMappingTable OBJECT-TYPE
		SYNTAX SEQUENCE OF MTP1TimeSlotMappingEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table maps the timeslots on an E1/T1 to a 
			specific processor.  This table is the Master
			table for all of MTP1 and MTP2.  This means that when
			configuration data is changed in any MTP1 or MTP2 table,
			the RowStatus variable in this table will display the
			current status of that change (whether it was accepted
			by the system (active), or a reboot needs to take place
			(pending), or the the change was rejected by the system
			(rejected)).

			Rows are added in this table by setting the rowStatus
			variable to Create and by specifying a valid value 
			for all other variables.  All variables must be
			specified in the SNMP-Set operation in order 
			for a row to be created.  

			Rows are deleted by in this table by setting the
			rowStatus variable to Destroy.  When a row in this
			table is deleted, then all MTP1 and MTP2 configuration 
			tables that share this same value for the index will 
			also be deleted.
			
			Rows can be updated in this table by setting the
			rowStatus variable to Update and by modifying the
			variables that need changed in that row."

		::= { mtp1 1}

	mtp1TimeSlotMappingEntry OBJECT-TYPE
		SYNTAX MTP1TimeSlotMappingEntry 
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
		    "An entry in the mtp1TimeSlotMapping table."
		INDEX { mtp1TimeSlotMappingSs7LinkIdIndex}
		::= { mtp1TimeSlotMappingTable 1 }

	MTP1TimeSlotMappingEntry ::=
		SEQUENCE
		{
	        	mtp1TimeSlotMappingSs7LinkIdIndex
				INTEGER(1..32),
			mtp1TimeSlotMappingRowStatus
				INTEGER,
			pnode
				INTEGER(1..8),
			lineId
				INTEGER(1..2),
			timeslotNumber
				INTEGER(0..31),
			bitrate
				INTEGER,
			ds0a
				Boolean

		}

	mtp1TimeSlotMappingSs7LinkIdIndex OBJECT-TYPE
		SYNTAX  INTEGER(1..32)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"index that uniquely identifies each SS7 link 
			.  This index value is used throughout MTP1
			and MTP2 tables to uniquely identify the same SS7 link
			It is also used in the MTP3Link Tables.  These
			values must be created in ascending, synchronous order."
                                                
		::= { mtp1TimeSlotMappingEntry 1}

	mtp1TimeSlotMappingRowStatus OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   create(1),
			   destroy(2),
			   update(3),
			   active(4),
			   pending(5),
			   rejected(6)
			}

		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The status of the row.
			The only valid writeable values are Create, Destroy 
			and Update. These are explained in the Table Definition.
			All other values are R/O and will be updated internally
			by the system. 
			Active - all variables in this table are in use by
			         the system
			Pending - a variable has been changed in this table
				  or another MTP1 or MTP2 configuration table 
				  and is not currently in use by the system.
				  The system must be rebooted before the
				  variable will take effect in the system.
			Rejected - a variable was changed in this table
				   before the last reboot.  That change was 
				   rejected by the system after the reboot. 
				   The variable needs to be retored to it's
				   previous value."
                                                
		::= { mtp1TimeSlotMappingEntry 2}

	pnode OBJECT-TYPE
		SYNTAX  INTEGER(1..8)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"physical node Id that this SS7 link is to be 
			connected to"
                                                
		::= { mtp1TimeSlotMappingEntry 3}

	lineId OBJECT-TYPE
		SYNTAX  INTEGER(1..2)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Id of the E1/T1 terminated on the processor"
                                                
		::= { mtp1TimeSlotMappingEntry 4}

	timeslotNumber OBJECT-TYPE
		SYNTAX  INTEGER(0..31)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"timeslot number on the E1/T1 that is terminated by 
			the processor"
                                                
		::= { mtp1TimeSlotMappingEntry 5}

	bitrate OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   bitrate-64K(1),
			   bitrate-56K(2),
			   bitrate-48K(3)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"signalling link bitrate."
                                                
		::= { mtp1TimeSlotMappingEntry 6}

	ds0a OBJECT-TYPE
		SYNTAX  Boolean
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"ds0a link"
                                                
		::= { mtp1TimeSlotMappingEntry 7}

