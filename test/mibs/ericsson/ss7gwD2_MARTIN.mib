			   enabled(1)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Enable/Disable load sharing between the links"
                                                
		::= { mtp3LinkSetConfigEntry 4}

	numlinks OBJECT-TYPE
		SYNTAX  INTEGER(1..16)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"number of links in the linkset"
                                                
		::= { mtp3LinkSetConfigEntry 5}

	commandsMTP3linkset OBJECT-TYPE
		SYNTAX  INTEGER
			{
			   disable(0),
			   enable(1)
			}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"command to be performed on the link set"
                                                
		::= { mtp3LinkSetConfigEntry 6}

